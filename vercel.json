{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/admin",
      "destination": "/admin.html"
    },
    {
      "source": "/test1",
      "destination": "/test1.html"
    },
    {
      "source": "/test2", 
      "destination": "/test2.html"
    },
    {
      "source": "/test3",
      "destination": "/test3.html"
    },
    {
      "source": "/result",
      "destination": "/result.html"
    },
    {
      "source": "/share",
      "destination": "/share.html"
    },
    {
      "source": "/game",
      "destination": "/game.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}